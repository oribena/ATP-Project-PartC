                  <Menu mnemonicParsing="false" onAction="#Exit" text="Exit" />
               </menus>
            </MenuBar>
         </top>
         <right>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#About" text="About" />
