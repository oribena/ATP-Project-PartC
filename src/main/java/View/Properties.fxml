<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="614.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.propertiesController">
    <children>
        <VBox alignment="CENTER" prefHeight="614.0" prefWidth="960.0">
            <children>
                <Label text="Maze Generator">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin></Label>
                <ComboBox fx:id="Generator" prefWidth="150.0">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin></ComboBox>
                <Label text="Searching Algorithm">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin></Label>
                <ComboBox fx:id="Search" prefWidth="150.0">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin></ComboBox>
                <Label text="Number Of Threads">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin></Label>
                <TextField fx:id="numOfT" maxWidth="150.0" prefHeight="27.0" prefWidth="30.0">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </VBox.margin></TextField>
                <Button fx:id="okB" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#OK" text="Set Properties" textAlignment="CENTER">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
                    </VBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets></Button>
            </children>
        </VBox>
      <Label layoutX="50.0" layoutY="100.0" text="Change properties:">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
    </children>
</AnchorPane>
